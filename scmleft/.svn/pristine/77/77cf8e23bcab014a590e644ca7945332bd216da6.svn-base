package kr.happyjob.study.scm.model;


public class WHInventoryFormModel {
	
	private int wh_no; //창고 넘버
	private int wh_code; // 창고 코드
	private int model_code; // 제품 번호
	private String wh_name; // 창고 명
	private String pd_name; // 제품 명
	private int wh_stock; // 재고 수량
	private String wh_addr;	// 창고 위치
	private int input; //입고량
	private int output; //출고량
	
	
	public int getWh_no() {
		return wh_no;
	}
	public void setWh_no(int wh_no) {
		this.wh_no = wh_no;
	}
	public int getWh_code() {
		return wh_code;
	}
	public void setWh_code(int wh_code) {
		this.wh_code = wh_code;
	}
	public int getModel_code() {
		return model_code;
	}
	public void setModel_code(int model_code) {
		this.model_code = model_code;
	}
	public String getWh_name() {
		return wh_name;
	}
	public void setWh_name(String wh_name) {
		this.wh_name = wh_name;
	}
	public String getPd_name() {
		return pd_name;
	}
	public void setPd_name(String pd_name) {
		this.pd_name = pd_name;
	}
	public int getWh_stock() {
		return wh_stock;
	}
	public void setWh_stock(int wh_stock) {
		this.wh_stock = wh_stock;
	}
	public String getWh_addr() {
		return wh_addr;
	}
	public void setWh_addr(String wh_addr) {
		this.wh_addr = wh_addr;
	}
	public int getInput() {
		return input;
	}
	public void setInput(int input) {
		this.input = input;
	}
	public int getOutput() {
		return output;
	}
	public void setOutput(int output) {
		this.output = output;
	}
	
	
	
}
